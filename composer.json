{
    "name": "lawoole/lawoole",
    "description": "The Lawoole framework",
    "keywords": ["framework", "swoole", "laravel", "lawoole"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "lawoole/framework": "0.1.*",
        "illuminate/view": "5.5.*",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "lawoole/swoole-api": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
